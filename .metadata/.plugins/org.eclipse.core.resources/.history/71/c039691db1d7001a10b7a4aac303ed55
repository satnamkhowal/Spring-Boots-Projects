package com.gs;

import java.util.Date;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ApplicationContext;

import com.gs.entities.Ticket;
import com.gs.service.TicketBookingService;

@SpringBootApplication
public class TicketBookingManagementAppApplication {

	public static void main(String[] args) {
				ApplicationContext ap=	SpringApplication.run(TicketBookingManagementAppApplication.class, args);
				
				
				TicketBookingService ticketBookingService=ap.getBean("TicketBookingService",TicketBookingService.class);
				
				
				Ticket ticket=new Ticket();
				ticket.setBookingDate(new Date());;
	}

}
